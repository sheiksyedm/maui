<Project>
  <!-- A list of dependencies from https://github.com/dotnet/android-libraries -->
  <ItemGroup>
    <!-- GLIDE - the android maven artifact in /src/Core/AndroidNative/maui/build.gradle -->
    <!-- must be kept in sync with the binding library version to it here: -->
    <PackageReference Update="Xamarin.Android.Glide" Version="5.0.5.1" />
    <PackageReference Update="Xamarin.AndroidX.Activity" Version="1.12.2.1" />
    <PackageReference Update="Xamarin.AndroidX.Browser" Version="1.9.0.1" />
    <PackageReference Update="Xamarin.AndroidX.DynamicAnimation" Version="1.1.0.4" />
    <PackageReference Update="Xamarin.AndroidX.Lifecycle.LiveData" Version="2.10.0.1" />
    <PackageReference Update="Xamarin.AndroidX.Navigation.Common" Version="2.9.6.1" />
    <PackageReference Update="Xamarin.AndroidX.Navigation.Fragment" Version="2.9.6.1" />
    <PackageReference Update="Xamarin.AndroidX.Navigation.Runtime" Version="2.9.6.1" />
    <PackageReference Update="Xamarin.AndroidX.Navigation.UI" Version="2.9.6.1" />
    <!-- Navigation 2.9.6.1 transitively pulls in NavigationEvent which depends on both
         Compose.Runtime.Annotation.Android and Compose.Runtime.Annotation.Jvm. Both contain
         androidx.compose.runtime.Immutable, causing a duplicate class error during D8/R8 dex
         compilation. Override as a direct reference with ExcludeAssets="all" so its buildTransitive
         targets (which add the JVM jar to AndroidJavaLibrary) are never evaluated. The Android AAR
         variant already provides these classes, so no functionality is lost. -->
    <PackageReference Include="Xamarin.AndroidX.Compose.Runtime.Annotation.Jvm" Version="1.10.0.1" ExcludeAssets="all" />
    <PackageReference Update="Xamarin.AndroidX.Security.SecurityCrypto" Version="1.1.0.4-alpha07" />
    <PackageReference Update="Xamarin.AndroidX.SwipeRefreshLayout" Version="1.2.0.1" />
    <PackageReference Update="Xamarin.AndroidX.Transition" Version="1.6.0.2" />
    <PackageReference Update="Xamarin.AndroidX.Window.WindowJava" Version="1.5.1.1" />
    <PackageReference Update="Xamarin.Firebase.AppIndexing" Version="120.0.0.27" />
    <PackageReference Update="Xamarin.Google.Android.Material" Version="1.13.0.1" />
    <PackageReference Update="Xamarin.Google.Crypto.Tink.Android" Version="1.20.0.1" />
    <PackageReference Update="Xamarin.GooglePlayServices.Maps" Version="119.2.0.4" />
  </ItemGroup>
</Project>